{
  "name": "genest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "copy:templates": "mkdir -p dist/templates && cp -R ./templates/dist ./dist/templates",
    "build": "rm -rf dist && tsc -p tsconfig.build.json && npm run copy:templates",
    "format": "prettier --write \"src/**/*.ts\"",
    "mjml": "mkdir -p templates/dist && mjml templates/**/*.mjml -o templates/dist/",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "env NODE_ENV=development tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "dev": "npm-run-all --parallel mjml start:dev",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/main.js",
    "watch": "nodemon --exec \"npm run build && npm run start\" --watch src --ext ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate-entities": "npx typeorm-model-generator -h localhost -d genesis -u genesis -e postgres -o ./.tmp -s public --cf pascal --ce pascal --cp camel --lazy"
  },
  "_moduleAliases": {
    "@app": "dist/app",
    "@modules": "dist/app/modules",
    "@config": "dist/config",
    "@core": "dist/core"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.2.1",
    "@nest-middlewares/csurf": "^6.0.0",
    "@nest-middlewares/express-session": "^6.0.1",
    "@nest-middlewares/helmet": "^6.0.0",
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.10.1",
    "@nestjs/core": "^6.10.1",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/swagger": "^4.1.12",
    "@nestjs/terminus": "^6.5.3",
    "@nestjs/typeorm": "^6.2.0",
    "@types/module-alias": "^2.0.0",
    "@types/redis": "^2.8.14",
    "apollo-server": "^2.9.9",
    "apollo-server-express": "^2.9.9",
    "argon2": "^0.25.1",
    "aws-sdk": "^2.602.0",
    "class-transformer": "^0.2.3",
    "compression": "^1.7.4",
    "connect-redis": "^4.0.3",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.10.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.0.0",
    "express-session": "^1.17.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "graphql-type-json": "^0.3.1",
    "helmet": "^3.21.2",
    "lodash.get": "^4.4.2",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "multer-ftp": "^1.2.0",
    "multer-s3": "^2.9.0",
    "nestjs-config": "^1.4.4",
    "nestjs-redis": "^1.2.3",
    "nodemailer": "^6.3.1",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.13.0",
    "randomatic": "^3.1.1",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "sharp": "^0.23.4",
    "type-graphql": "^0.17.5",
    "typeorm": "^0.2.20"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.10.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/multer": "^1.3.10",
    "@types/node": "^12.12.11",
    "@types/nodemailer": "^6.2.2",
    "@types/passport-facebook-token": "^0.4.33",
    "@types/passport-github": "^1.1.5",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/passport-twitter": "^1.0.34",
    "@types/supertest": "^2.0.8",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "mjml": "^4.5.1",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.2",
    "tsc-watch": "4.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "5.20.1",
    "typescript": "^3.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
